import java.awt.Color;
import java.awt.Graphics;

public class Goomba 
extends GameObject{
	
	Goomba(int x, int y, int width, int height){
		this.x=x;
		this.y=y;
		this.width=width;
		this.height=height;
	}
	
	void update(){
		
	}
	
	void draw(Graphics g){
		g.setColor(Color.BLACK);
		g.fillRect(x, y, width, height);
	}

}
